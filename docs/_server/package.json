{
  "name": "edge-ai-progress-server",
  "version": "2.0.0",
  "type": "module",
  "description": "Express.js progress tracking server with SSE and schema validation",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:integration": "vitest run --config vitest.integration.config.js",
    "lint": "cd ../.. && npm run eslint:server",
    "lint:fix": "cd ../.. && npm run eslint:server -- --fix"
  },
  "dependencies": {
    "ajv": "^8.17.1",
    "ajv-formats": "^2.1.1",
    "chokidar": "^3.6.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.22.1",
    "express-rate-limit": "^6.7.0",
    "fast-glob": "^3.3.3",
    "gray-matter": "^4.0.3",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@vitest/coverage-v8": "^4.0.13",
    "@vitest/ui": "^4.0.13",
    "eslint": "^9.32.0",
    "eventsource": "^4.0.0",
    "happy-dom": "^20.0.3",
    "node-fetch": "^3.3.2",
    "nodemon": "^3.0.1",
    "supertest": "^7.1.4",
    "vitest": "^4.0.13"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "overrides": {
    "js-yaml": "^4.1.0"
  },
  "license": "MIT",
  "keywords": [
    "express",
    "server",
    "progress-tracking",
    "sse",
    "api",
    "schema-validation"
  ]
}
