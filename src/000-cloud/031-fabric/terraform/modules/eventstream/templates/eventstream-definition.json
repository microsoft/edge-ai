{
  "source": {
    "type": "AzureEventGrid",
    "properties": {
      "endpoint": "${EventGridEndpoint}",
      "authentication": {
        "type": "ManagedIdentity"
      }
    }
  },
  "destination": {
    "type": "Lakehouse",
    "properties": {
      "workspaceId": "${WorkspaceID}",
      "lakehouseId": "${LakehouseID}",
      "tableName": "eventstream_data",
      "format": "Parquet",
      "compressionType": "Snappy"
    }
  },
  "processing": {
    "timestampColumn": "EventTime",
    "watermarkColumn": "EventTime",
    "watermarkDelay": "PT5M"
  }
}
